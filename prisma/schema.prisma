datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Event {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  thumb  String
  title  String
  image  String
  author String
  data   String
  text   String
}
